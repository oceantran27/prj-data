class Solution {
public:
    bool increasingTriplet(vector<int>& nums) {
        int n = nums.size();
    if(n<3) return false;
    long long var1= nums[0],var2=3e9;
        for(int i=1;i<n;i++){
              if(nums[i]>var2){
                return true;
            }
            if(nums[i]>var1){
                var2 = nums[i];
            }
            else if(nums[i]<var1){
                var1 = nums[i];
            }
          
        }
        return false;
    }
};
TC- O(n) , SC- O(1);
